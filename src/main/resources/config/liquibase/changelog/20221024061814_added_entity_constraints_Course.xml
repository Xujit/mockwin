<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Course.
    -->
    <changeSet id="20221024061814-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="exam_type_id"
                                 baseTableName="course"
                                 constraintName="fk_course_exam_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="exam_type"/>

        <addForeignKeyConstraint baseColumnNames="compet_exam_id"
                                 baseTableName="course"
                                 constraintName="fk_course_compet_exam_id"
                                 referencedColumnNames="id"
                                 referencedTableName="compet_exam"/>

        <addForeignKeyConstraint baseColumnNames="compet_exam_response_id"
                                 baseTableName="course"
                                 constraintName="fk_course_compet_exam_response_id"
                                 referencedColumnNames="id"
                                 referencedTableName="compet_exam_response"/>

    </changeSet>
</databaseChangeLog>
